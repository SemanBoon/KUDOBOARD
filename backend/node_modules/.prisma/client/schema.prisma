generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model KudosBoard {
  id       Int    @id @unique @default(autoincrement())
  imgUrl   String
  title    String
  category String
}

model KudosCard {
  id       Int    @id @unique @default(autoincrement())
  imgUrl   String
  title    String
  category String
}
